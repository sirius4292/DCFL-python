k=int(input(print('Kasım ayı içinkaç gider gireceksiniz:')))
while k>=1:
  kasımg=input(print('Gider ismi:'))
  kasımm=input(print('Gider miktarı: '))
  kasım1.append(kasımg)
  kasım1.append(kasımm)
  k=k-1
l=int(input(print('Aralık ayı içinkaç gider gireceksiniz:')))
while l>=1:
  aralıkg=input(print('Gider ismi:'))
  aralıkm=input(print('Gider miktarı: '))
  aralık1.append(aralıkg)
  aralık1.append(aralıkm)
  l=l-1
r=input(print('Görmek istediğiniz ayı yazın\nocak-subat-mart-nisan-mayıs-haziran-temmuz-agustos-eylul-ekim-kasım\nGeliriniz altta yazacaktır:'))
if r== 'ocak':
      print(ocak1)
      print(maas)
elif r== 'subat':
      print(subat1)
      print(maas)
elif r== 'mart':
      print(mart1)
      print(maas)
elif r== 'nisan':
      print(nisan1)
      print(maas)
elif r== 'mayıs':
      print(mayıs1)
      print(maas)
elif r== 'haziran':
      print(haziran1)
      print(maas)
elif r== 'temmuz':
      print(temmuz1)
      print(maas)
elif r== 'agustos':
      print(agustos1)
      print(maas)
elif r== 'eylul':
      print(eylul1)
      print(maas)
elif r== 'ekim':
      print(ekim1)
      print(maas)
elif r== 'kasım':
      print(kasım1)
      print(maas)
elif r== 'aralık':
      print(aralık1)
      print(maas)
